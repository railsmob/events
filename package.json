  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "babel-jest": "^24.9.0",
    "eslint": "^6.7.1",
    "flow-bin": "^0.113.0",
    "flowgen": "^1.10.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "rollup": "^1.27.5",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.1.3",
    "typescript": "^3.7.3"
  },
  "jest": {
    "testEnvironment": "node"
  }
